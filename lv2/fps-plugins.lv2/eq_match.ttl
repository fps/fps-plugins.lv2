@prefix lv2:  <http://lv2plug.in/ns/lv2core#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix bufsize: <https://lv2plug.in/ns/ext/buf-size#> .
@prefix state: <http://lv2plug.in/ns/ext/state#> .
@prefix work:  <http://lv2plug.in/ns/ext/worker#> .
@prefix urid:  <http://lv2plug.in/ns/ext/urid#> .

<https://dfdx.eu/fps-plugins.lv2/eq_match> a lv2:Plugin ;
  doap:name "EQ match";
  doap:license <http://usefulinc.com/doap/licenses/gpl> ;
  doap:description "EQ matching of two signals" ;
  lv2:binary <eq_match.so> ;
  lv2:optionalFeature lv2:hardRTCapable ;
  lv2:requiredFeature urid:map,
    work:schedule ;
  lv2:extensionData state:interface,
    work:interface ;
  lv2:port [
    a lv2:AudioPort ;
    a lv2:InputPort ;
    lv2:index 0 ;
    lv2:symbol "in" ;
    lv2:name "In"
  ] , [
    a lv2:AudioPort ;
    a lv2:OutputPort ;
    lv2:index 1 ;
    lv2:symbol "out" ;
    lv2:name "Out"
  ] , [
    a lv2:InputPort ;
    a lv2:ControlPort ;
    lv2:index 2 ;
    lv2:symbol "analyze1" ;
    lv2:name "Analyze spectrum 1";
    lv2:default 0 ;
    lv2:minimum 0 ;
    lv2:maximum 1 ;
    lv2:portProperty lv2:toggled ;
  ] , [
    a lv2:InputPort ;
    a lv2:ControlPort ;
    lv2:index 3 ;
    lv2:symbol "analyze2" ;
    lv2:name "Analyze spectrum 2";
    lv2:default 0 ;
    lv2:minimum 0 ;
    lv2:maximum 1 ;
    lv2:portProperty lv2:toggled ;
  ] , [
    a lv2:InputPort ;
    a lv2:ControlPort ;
    lv2:index 4 ;
    lv2:symbol "apply" ;
    lv2:name "Apply spectrum 2 / spectrum 1";
    lv2:default 0 ;
    lv2:minimum 0 ;
    lv2:maximum 1 ;
    lv2:portProperty lv2:toggled ;
  ] , [
    a lv2:InputPort ;
    a lv2:ControlPort ;
    lv2:index 5 ;
    lv2:symbol "minimum_phase" ;
    lv2:name "Minimum phase";
    lv2:default 1 ;
    lv2:minimum 0 ;
    lv2:maximum 1 ;
    lv2:portProperty lv2:toggled ;
  ] , [
    a lv2:InputPort ;
    a lv2:ControlPort ;
    lv2:index 6 ;
    lv2:symbol "gain" ;
    lv2:name "Gain (dB)";
    lv2:default 0 ;
    lv2:minimum -72.0 ;
    lv2:maximum 36.0 ;
  ] .
