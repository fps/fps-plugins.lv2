@prefix lv2:  <http://lv2plug.in/ns/lv2core#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix doap: <http://usefulinc.com/ns/doap#> .

<http://dfdx.eu/plugins/eq_match> a lv2:Plugin ;
  doap:name "EQ match";
  doap:license <http://usefulinc.com/doap/licenses/gpl> ;
  doap:description "EQ matching of two signals" ;
  lv2:binary <eq_match.so> ;
  lv2:optionalFeature lv2:hardRTCapable ;
  lv2:port [
    a lv2:AudioPort ;
    a lv2:InputPort ;
    lv2:index 0 ;
    lv2:symbol "in" ;
    lv2:name "In"
  ] , [
    a lv2:AudioPort ;
    a lv2:OutputPort ;
    lv2:index 1 ;
    lv2:symbol "out" ;
    lv2:name "Out"
  ] , [
    a lv2:InputPort ;
    a lv2:ControlPort ;
    lv2:index 2 ;
    lv2:symbol "analyze1" ;
    lv2:name "Analyze spectrum 1";
    lv2:default 0 ;
    lv2:minimum -1 ;
    lv2:maximum 1 ;
  ]  , [
    a lv2:InputPort ;
    a lv2:ControlPort ;
    lv2:index 3 ;
    lv2:symbol "analyze2" ;
    lv2:name "Analyze spectrum 2";
    lv2:default 0 ;
    lv2:minimum -1 ;
    lv2:maximum 1 ;
  ]  , [
    a lv2:InputPort ;
    a lv2:ControlPort ;
    lv2:index 4 ;
    lv2:symbol "apply" ;
    lv2:name "Apply spectrum 2 / spectrum 1";
    lv2:default 0 ;
    lv2:minimum -1 ;
    lv2:maximum 1 ;
  ]  .
