@prefix lv2:  <http://lv2plug.in/ns/lv2core#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix doap: <http://usefulinc.com/ns/doap#> .

<https://dfdx.eu/fps-plugins.lv2/stereo-decorrelation> a lv2:Plugin ;
  doap:name "stereo spreader (stereo to stereo)";
  doap:license <http://usefulinc.com/doap/licenses/gpl> ;
  doap:description "Random white noise exponential fall-off FIR filters for left and right" ; 
  lv2:binary <stereo_decorrelation.so> ;
  lv2:optionalFeature lv2:hardRTCapable ;
  lv2:port [
    a lv2:AudioPort ;
    a lv2:InputPort ;
    lv2:index 0 ;
    lv2:symbol "inl" ;
    lv2:name "InL"
  ] , [
    a lv2:AudioPort ;
    a lv2:InputPort ;
    lv2:index 1 ;
    lv2:symbol "inr" ;
    lv2:name "InR"
  ] , [
    a lv2:AudioPort ;
    a lv2:OutputPort ;
    lv2:index 2 ;
    lv2:symbol "outl" ;
    lv2:name "Out Left"
  ] , [
    a lv2:AudioPort ;
    a lv2:OutputPort ;
    lv2:index 3 ;
    lv2:symbol "outr" ;
    lv2:name "Out Right"
  ] , [
    a lv2:InputPort ;
    a lv2:ControlPort ;
    lv2:index 4 ;
    lv2:symbol "drywet" ;
    lv2:name "Dry/Wet";
    lv2:default 1 ;
    lv2:minimum 0 ;
    lv2:maximum 1 ;
  ] , [
    a lv2:InputPort ;
    a lv2:ControlPort ;
    lv2:index 5 ;
    lv2:symbol "length" ;
    lv2:name "Filter length (s)";
    lv2:default 3 ;
    lv2:minimum 0 ;
    lv2:maximum 8 ;
  ] , [
    a lv2:InputPort ;
    a lv2:ControlPort ;
    lv2:index 6 ;
    lv2:symbol "decay" ;
    lv2:name "Filter decay (s)";
    lv2:default 3 ;
    lv2:minimum 0 ;
    lv2:maximum 8 ;
  ] , [
    a lv2:InputPort ;
    a lv2:ControlPort ;
    lv2:index 7 ;
    lv2:symbol "seed" ;
    lv2:name "Random seed";
    lv2:default 0 ;
    lv2:minimum 0 ;
    lv2:maximum 1024 ;
  ] .
